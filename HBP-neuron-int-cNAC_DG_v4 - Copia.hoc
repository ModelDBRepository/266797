proc change_kin(){
print "change_all_kin"
	forall{
	// Nax
	tha_nax=-31
	qa_nax=5.1
	thinf_nax=-58.2
	qinf_nax=5


	/*Kdr
	vhalfn_kdr=-3.8
	zetan_kdr=-2.4
	*/
